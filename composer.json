{
  "name": "laravelrus/sleepingowl",
  "description": "Administrative interface builder for Laravel.",
  "homepage": "http://sleeping-owl.github.io",
  "keywords": ["laravel", "admin", "administrator", "crud"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/LaravelRUS/SleepingOwlAdmin/issues",
    "source": "https://github.com/LaravelRUS/SleepingOwlAdmin"
  },
  "authors": [
    {
      "name": "Sleeping Owl",
      "email": "owl.sleeping@yahoo.com"
    },
    {
      "name": "Pavel Buchnev",
      "email": "butschster@gmail.com",
      "homepage": "http://www.kodicms.ru/"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.*",
    "kodicomponents/support": "0.*",
    "kodicomponents/navigation": "0.*",
    "doctrine/dbal": "~2.3",
    "laravelcollective/html": "5.*",
    "kodicms/laravel-assets": "0.*",
    "erusev/parsedown": "1.*",
    "davejamesmiller/laravel-breadcrumbs": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1",
    "mockery/mockery": "^0.9.4",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3",
    "orchestra/testbench": "3.0.x|3.1.x|3.2.x"
  },
  "autoload": {
    "psr-4": {
      "SleepingOwl\\Admin\\": "src/"
    }
  },
  "autoload-dev": {
    "autoload-dev": {
      "psr-4": {
        "SleepingOwl\\Admin\\": "tests"
      }
    }
  },
  "extra": {
    "branch-alias": {
      "dev-development": "4.x-dev"
    }
  }
}
